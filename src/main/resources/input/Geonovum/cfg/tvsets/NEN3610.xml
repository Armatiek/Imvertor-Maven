<tagset type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <!-- 
        Geonovum NEN3610 follows KKG-ISO-EN
    --> 
    <xi:include href="../../../KKG/cfg/tvsets/KKG-ISO.xml"/>
    
    <name>NEN3610</name>
    <desc lang="en">Tagset for NEN3610 conformant KKG-ISO extensions</desc>
    <desc lang="nl">Tags voor NEN3610 vereiste KKG-ISO uitbreidingen</desc>
    
    <tagged-values>
        <tv norm="space" id="CFG-TV-DATERECORDED">
            <derive>yes</derive>
            <stereotypes>
                <stereo minmax="0..1">stereotype-name-relatieklasse</stereo>
            </stereotypes>
        </tv>
    </tagged-values>
    
    <tagged-values>
        <tv norm="space" id="CFG-TV-XSDLOCATION">
            <name lang="nl">XML Schema locatie</name>
            <name lang="en">XML Schema location</name>
            <desc lang="nl">De URL op basis waarvan de locatie van het corresponderende XML schema kan worden bepaald</desc>
            <desc lang="en">The URL based on which the location of the XML Schema can be determined</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo minmax="0..1">stereotype-name-domain-package</stereo>
            </stereotypes>
        </tv>
    </tagged-values>
    
</tagset>