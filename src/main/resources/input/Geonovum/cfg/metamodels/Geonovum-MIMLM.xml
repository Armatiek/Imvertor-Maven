<metamodel type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    <!-- 
        Geonovum follows NEN3610
        
        Including scalars.
    --> 
    <xi:include href="../../../MIM/cfg/metamodels/MIM11-ISO.xml"/>
    
    <name>Geonovum MIMLM</name>
    
    <profiles>
        <profile lang="nl"> 
            <id>NBGNL</id>
            <release>0.9.3</release>
            <desc>NEN3610 BRO Grouping NL</desc> 
        </profile>
        <profile lang="en">
            <id>NBGEN</id>
            <release>0.9.3</release>
            <desc>NEN3610 BRO Grouping EN</desc>
        </profile>
    </profiles>
    
    <naming>
        <package>
            <format>AsIs</format>
        </package>
        <class>
            <format>AsIs</format>
        </class>
        <property>
            <format>AsIs</format>
        </property>
        <!-- tv conventions as defined elsewhere -->
    </naming>
    
    <features>
        <feature name="allow-multiple-supertypes">false</feature>
        <feature name="allow-scalar-in-union">true</feature>
        <feature name="unique-normalized-class-names">domain</feature>
    </features>
    
</metamodel>