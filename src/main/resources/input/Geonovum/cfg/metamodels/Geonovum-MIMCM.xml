<metamodel type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    <!-- 
        Geonovum follows MIM10 ISO (alt 2)
        
        Including scalars.
    --> 
    <xi:include href="../../../MIM/cfg/metamodels/MIM10.xml"/>
    
    <name>Geonovum MIMCM</name>
    
    <profiles>
        <profile lang="nl"> 
            <id>MCGGNL</id>
            <!-- <release/> identical to Imvertor release -->
            <desc>MIMCM Geonovum Grouping NL</desc> 
        </profile>
        <profile lang="en"> <!-- ACTUALLY, there is no english variant -->
            <id>MCGGEN</id>
            <!-- <release/> identical to Imvertor release -->
            <desc>MIMCM Geonovum Grouping EN</desc>
        </profile>
    </profiles>
    
    <features>
        <feature name="allow-multiple-supertypes">true</feature>
        <feature name="allow-scalar-in-union">true</feature>
    </features>
    
    <stereotypes xml:id="stereotypes">
        <stereo id="stereotype-name-codelist">
            <name lang="nl">Codelist</name>
        </stereo>       
    </stereotypes>
    
</metamodel>
