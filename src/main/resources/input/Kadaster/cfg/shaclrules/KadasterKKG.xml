<shacl-rules type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    <name>Kadaster KKG</name>
    <!-- 
        Kadaster KKG specific SHACL rules 
    -->
    <vocabularies>
        <vocabulary>
            <prefix>rdf</prefix>
            <URI>http://www.w3.org/1999/02/22-rdf-syntax-ns#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>rdfs</prefix>
            <URI>http://www.w3.org/2000/01/rdf-schema#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>xsd</prefix>
            <URI>http://www.w3.org/2001/XMLSchema#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>owl</prefix>
            <URI>http://www.w3.org/2002/07/owl#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>sh</prefix>
            <URI>http://www.w3.org/ns/shacl#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dcat</prefix>
            <URI>http://www.w3.org/ns/dcat#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dct</prefix>
            <URI>http://purl.org/dc/terms/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dctype</prefix>
            <URI>http://purl.org/dc/dcmitype/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>foaf</prefix>
            <URI>http://xmlns.com/foaf/0.1/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>skos</prefix>
            <URI>http://www.w3.org/2004/02/skos/core#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>skoslex</prefix>
            <URI>http://bp4mc2.org/def/skos-lex/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>prov</prefix>
            <URI>http://www.w3.org/ns/prov#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dqv</prefix>
            <URI>http://www.w3.org/ns/dqv#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>adms</prefix>
            <URI>http://www.w3.org/ns/adms#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>wdrs</prefix>
            <URI>http://www.w3.org/2007/05/powder-s#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>xsi</prefix>
            <URI>http://www.w3.org/2001/XMLSchema-instance</URI>
        </vocabulary>
        <vocabulary>
            <prefix>ldqd</prefix>
            <URI>http://www.w3.org/2016/05/ldqd#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>kkg</prefix>
            <URI>http://data.acceptatie.pdok.nl/KKG/def/model/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>data</prefix>
            <URI>http://data.ontwikkeling.pdok.nl/catalogus/dso/id/model#</URI>
        </vocabulary>

    </vocabularies>
    
    <node-mapping>
        <!-- 
            Alleen de tagged values die hier zijn opgenomen worden omgezet naar een regel in TTL Shacl 
            Dit worden constructies zoals kkg:definitie '''Identifier or geographic name.....''';
	    -->
        <map id="CFG-TV-DEFINITION" type="3q" lang="#all">kkg:definitie</map>
        <map id="CFG-TV-DESCRIPTION" type="2q" lang="#all">kkg:toelichting</map>
        <map id="CFG-TV-POPULATION" type="2q" lang="#all">kkg:populatie</map>
        <map id="CFG-TV-LENGTH" type="2q" lang="#all">kkg:lengte</map>
        <map id="CFG-TV-SOURCE" type="2q" lang="#all">kkg:herkomst</map>
        <map id="CFG-TV-INDICATIONAUTHENTIC" type="2q" lang="#all">kkg:indicatieAuthentiek</map>
        <map id="CFG-TV-VOIDABLE" type="2q" lang="#all">kkg:mogelijkGeenWaarde</map>
        <map id="CFG-TV-INDICATIONFORMALHISTORY" type="2q" lang="#all">kkg:indicatieFormeleHistorie</map>
        <map id="CFG-TV-INDICATIONMATERIALHISTORY" type="2q" lang="#all">kkg:indicatieMaterieleHistorie</map>
        <map id="CFG-TV-DATERECORDED" type="2q" lang="#all">kkg:datumOpname</map>
        <map id="CFG-TV-SOURCEOFDEFINITION" type="2q" lang="#all">kkg:herkomstDefinitie</map>
      
        
        
        
    </node-mapping>
</shacl-rules>
