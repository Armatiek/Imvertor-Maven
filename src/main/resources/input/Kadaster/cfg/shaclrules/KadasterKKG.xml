<shacl-rules>
    <name>Kadaster KKG</name>
    <!-- 
        Kadaster specific SHACL rules 
    -->
    <vocabularies>
        <vocabulary>
            <prefix>rdf</prefix>
            <URI>http://www.w3.org/1999/02/22-rdf-syntax-ns#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>rdfs</prefix>
            <URI>http://www.w3.org/2000/01/rdf-schema#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>xsd</prefix>
            <URI>http://www.w3.org/2001/XMLSchema#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>owl</prefix>
            <URI>http://www.w3.org/2002/07/owl#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>sh</prefix>
            <URI>http://www.w3.org/ns/shacl#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dcat</prefix>
            <URI>http://www.w3.org/ns/dcat#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dct</prefix>
            <URI>http://purl.org/dc/terms/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>dctype</prefix>
