        </vocabulary>
        <vocabulary>
            <prefix>ldqd</prefix>
            <URI>http://www.w3.org/2016/05/ldqd#</URI>
        </vocabulary>
        <vocabulary>
            <prefix>kkg</prefix>
            <URI>http://data.acceptatie.pdok.nl/KKG/def/model/</URI>
        </vocabulary>
        <vocabulary>
            <prefix>data</prefix>
            <URI>http://data.ontwikkeling.pdok.nl/catalogus/dso/id/model#</URI>
        </vocabulary>

    </vocabularies>
</shacl-rules>
