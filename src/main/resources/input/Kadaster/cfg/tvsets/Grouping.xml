<tagset type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="../../../Imvertor/cfg/tvsets/Grouping.xml"/>
    
    <name>Grouping for Kadaster</name>
  
    <desc>Deviations and additions to generic grouping for Kadaster metamodel.</desc>  
   
    <tagged-values>
        
        <tv norm="space" id="CFG-TV-DATALOCATION">
            <!-- bepaal de naam van deze tagged value hier, en ken toe aan nieuwe stereo's -->
            <name lang="nl">Data locatie</name>
            <name lang="en">Data location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-FEATURESET">
            <name lang="nl">Objectenverzameling</name>
            <name lang="en">Featureset</name>
            <desc lang="nl">Naam van de verzameling van objecttypen. 
                Als deze tagged value is opgegeven wordt een Feature Collection met die naam aangemaakt, met featureMembers.
            </desc>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-domain-package</stereo>
            </stereotypes>
        </tv>
        
    </tagged-values>

   
</tagset>