<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <name>Kadaster</name>
    <tagged-values>
        
        <!-- kadaster follows BP requirements -->
        <xi:include href="../../../BP/cfg/tvsets/BP.xml"/>
        
        <tv norm="space" id="DatumOpname">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" rules="NOC" id="IndicatieMateriÃ«leHistorie">
           <derive>yes</derive>
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <!-- declared-values inherited -->
        </tv>
        <tv norm="space" rules="NOC" id="IndicatieFormeleHistorie">
            <derive>yes</derive>
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <!-- declared-values inherited -->
        </tv>
        <tv norm="space" id="Eigenaar">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Populatie">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Kwaliteit">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Gebruiksvoorwaarden">
            <name lang="nl">Gebruiksvoorwaarden</name>
            <name lang="en">Conditions of use</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Herkomst">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-HERKOMSTDEFINITIE">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Wetgeving">
            <name lang="nl">Wetgeving</name>
            <name lang="en">Legislation</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Toegankelijkheid">
            <name lang="nl">Toegankelijkheid</name>
            <name lang="en">Accessibility</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="MogelijkGeenWaarde">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-voidable</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="IndicatieAuthentiek">
            <derive>yes</derive>
            <stereotypes>
                <!-- optuonal for inherited stereos -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <!-- and add this stereo -->
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="BegripLocatie">
            <name lang="nl">Begrip locatie</name>
            <name lang="en">Concept location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="DataLocatie">
            <name lang="nl">Data locatie</name>
            <name lang="en">Data location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="BaseMapping">
            <name lang="nl">Base-mapping</name>
            <name lang="en">Base-mapping</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Relation">
            <name lang="nl">Relatie</name>
            <name lang="en">Relation</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="IncludeBDMH">
            <name lang="#all">Include-bdmh</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-service</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value default="yes">Ja</value>
                <value>Nee</value>
            </declared-values>
        </tv>
        
        <tv norm="space" id="IncludeLog">
            <name lang="#all">Include-log</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-service</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value default="yes">Ja</value>
                <value>Nee</value>
            </declared-values>
        </tv>
  
        <tv norm="space" id="IncludeProces">
            <name lang="#all">Include-proces</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-service</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value default="yes">Ja</value>
                <value>Nee</value>
            </declared-values>
        </tv>
        
    </tagged-values>
</tagset>