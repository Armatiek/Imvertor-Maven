<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <name>Kadaster</name>
    <tagged-values>
        
        <!-- kadaster follows BP requirements -->
        <xi:include href="../../../BP/cfg/tvsets/BP.xml"/>
        
        <tv norm="space" id="CFG-TV-DATERECORDED">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" rules="NOC" id="CFG-TV-INDICATIONMATERIALHISTORY">
           <derive>yes</derive>
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <!-- declared-values inherited -->
        </tv>
        <tv norm="space" rules="NOC" id="CFG-TV-INDICATIONFORMALHISTORY">
            <derive>yes</derive>
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <!-- declared-values inherited -->
        </tv>
        <tv norm="space" id="CFG-TV-ONWER">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-POPULATION">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-QUALITY">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-CONDITIONSOFUSE">
            <name lang="nl">Gebruiksvoorwaarden</name>
            <name lang="en">Conditions of use</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-SOURCE">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-SOURCEOFDEFINITION">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-LEGISLATION">
            <name lang="nl">Wetgeving</name>
            <name lang="en">Legislation</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-ACCESSIBILITY">
            <name lang="nl">Toegankelijkheid</name>
            <name lang="en">Accessibility</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-VOIDABLE">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-voidable</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-INDICATIONAUTHENTIC">
            <derive>yes</derive>
            <stereotypes>
                <!-- optuonal for inherited stereos -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-CONCEPTLOCATION">
            <name lang="nl">Begrip locatie</name>
            <name lang="en">Concept location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-DATALOCATION">
            <name lang="nl">Data locatie</name>
            <name lang="en">Data location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-BASEMAPPING">
            <name lang="nl">Base-mapping</name>
            <name lang="en">Base-mapping</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-RELATION">
            <name lang="nl">Relatie</name>
            <name lang="en">Relation</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-FORMALPATTERN">
            <name lang="nl">Patroon</name>
            <name lang="en">Pattern</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-simpletype</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-ENVELOPEMETHOD">
            <name lang="nl">Methode</name>
            <name lang="en">Method</name>
            <desc lang="nl">De verwerkingsmethode bij het samenstellen van de "envelop" van een schema. Er zijn twee varianten, SAE en BDS.</desc>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-service</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value default="yes">BDS</value>
                <value default="no">SAE</value>
            </declared-values>
        </tv>
        
    </tagged-values>
</tagset>