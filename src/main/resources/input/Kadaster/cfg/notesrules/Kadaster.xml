<notes-rules xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <name>Kadaster</name>
    
    <desc>Kadaster only allows definitions to occur in notes field.</desc>
    
    <notes-rule lang="nl" default="Definitie">
        <section title="Definitie" tagged-value="CFG-TV-DEFINITION">
            <label title="URI"/><!-- should be single URI -->
        </section>
    </notes-rule>
    
    <notes-rule lang="en" default="Definition">
        <section title="Definition" tagged-value="CFG-TV-DEFINITION">
            <label title="URI"/><!-- should be single URI -->
        </section>
    </notes-rule>
    
</notes-rules>