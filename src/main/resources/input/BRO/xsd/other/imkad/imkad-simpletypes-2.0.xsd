<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:kad="http://www.kadaster.nl/schemas/imkad"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.kadaster.nl/schemas/imkad"
           version="1.0">
   <xs:simpleType name="AN4">
      <xs:restriction base="xs:string">
         <xs:maxLength value="4"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="Waardenlijst">
      <xs:annotation>
         <xs:documentation>De toegestane waarden staan in een externe waardenlijst. De desbetreffende waardenlijst wordt gerefereerd in de documentatie. Elementen met dit type mogen niet leeg gelaten worden.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:pattern value="(\p{L}|\d|')+.*"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="KadID">
      <xs:restriction base="xs:string">
         <xs:pattern value="NL\.IMKAD\.[\d]{15}"/>
      </xs:restriction>
   </xs:simpleType>
