<metamodel type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <name>MIM 1.0 ISO</name>
    
    <desc lang="en">Metamodel voor MIM10 ISO uitwisselingsmodellen</desc>
    <desc lang="nl">Metamodel for MIM10 ISO exchange models</desc>
    
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="MIM10.xml"/>
   
    <profiles>
        <profile lang="nl">
            <id>MIM10ISONL</id>
            <!-- <release/> identical to Imvertor release -->
            <desc>MIM10-ISO Grouping (NL)</desc> 
        </profile>
        <profile lang="en">
            <id>MIM10ISOEN</id>
            <!-- <release/> identical to Imvertor release -->
            <desc>MIM10-ISO Grouping (EN)</desc>
        </profile>
    </profiles>
    
    <naming>
        <package>
            <format>UpperCamel</format>
        </package>
        <class>
            <format>UpperCamel</format>
        </class>
        <property>
            <format>LowerCamel</format>
        </property>
        <tv>
            <format>lowercase</format>
        </tv>
    </naming>
    
    <features>
        <feature name="role-based">true</feature>
    </features>
    
    <stereotypes xml:id="stereotypes">
                
    </stereotypes>
</metamodel>
