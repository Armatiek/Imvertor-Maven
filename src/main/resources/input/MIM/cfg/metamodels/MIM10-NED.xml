<metamodel type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <name>MIM 1.0 ISO</name>
    
    <desc lang="en">Metamodel voor MIM10 uitwisselingsmodellen</desc>
    <desc lang="nl">Metamodel for MIM10 exchange models</desc>
    
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="MIM10.xml"/>
   
    <profiles>
        <profile lang="nl">
            <id>MIM10NEDNL</id>
            <!-- <release/> identical to Imvertor release -->
            <desc>MIM10-NED Grouping (NL)</desc> 
        </profile>
        <profile lang="en">
            <id>MIM10NEDEN</id>
            <!-- <release/> identical to Imvertor release -->
            <desc>MIM10-NED Grouping (EN)</desc>
        </profile>
    </profiles>
    
    <naming>
        <package>
            <format>UpperCamel</format>
        </package>
        <class>
            <format>UpperCamel</format>
        </class>
        <property>
            <format>LowerCamel</format>
        </property>
        <tv>
            <format>lowercase</format>
        </tv>
    </naming>
    
    <features>
        <feature name="role-based">true</feature>
    </features>
    
    <stereotypes xml:id="stereotypes">

        <stereo id="stereotype-name-relatiesoort">
            <name lang="nl">Relatiesoort</name>
            <name lang="en">AssociationType</name>
            <desc lang="en">The type to which an associtation may belong.</desc>
            <desc lang="nl">De soort waartoe een relatie kan behoren.</desc>
            <construct>association</construct>
        </stereo>
      
    </stereotypes>
</metamodel>
