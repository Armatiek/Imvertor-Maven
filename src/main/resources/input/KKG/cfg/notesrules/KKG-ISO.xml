<notes-rules xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <name>KKG-ISO</name>
    
    <desc>These are the schemes to apply to notes fields in order to determine the tagged values.</desc>
    
    <notes-rule lang="en" default="Definition">
        <!-- when nothing marked, assume Description -->
        
        <section title="Name" tagged-value="CFG-TV-NAME"/>
        <section title="Definition" tagged-value="CFG-TV-DEFINITION">
            <!-- no labels allowed -->
        </section>
        <section title="Description" tagged-value="CFG-TV-DESCRIPTION">
            <label title="SOURCE" tagged-value="Herkomst"/>
            <label title="NOTE"/>
            <label title="EXAMPLE"/>
            <label title="DEFINITION"/>
            <label title="URI"/>
        </section>
    </notes-rule>

</notes-rules>