<tagset type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <name>KKG-ISO</name>
    <desc lang="en">Metamodel for ISO conformant KKG extensions</desc>
    <desc lang="nl">Metamodel voor ISO vereiste KKG uitbreidingen</desc>
    
    <desc> This is the profile of KKG for ISO compliance.</desc>
    
    <tagged-values>
        
        <xi:include href="../../../KKG/cfg/tvsets/KKG.xml"/>
        
        <tv id="CFG-TV-DATERECORDED">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv> 
        <tv id="CFG-TV-INDICATIONMATERIALHISTORY">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        <tv id="CFG-TV-INDICATIONFORMALHISTORY">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        <tv id="CFG-TV-INDICATIONAUTHENTIC">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        <tv id="CFG-TV-VOIDABLE">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        <tv id="CFG-TV-SOURCE">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        <tv id="CFG-TV-SOURCEOFDEFINITION">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        <tv id="CFG-TV-DESCRIPTION">
            <extract-from-notes>yes</extract-from-notes><!-- value may be read from notes field usig KKG-ISO scheme -->
            <stereotypes>
                <stereo required="no">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>  
        
        <tv id="CFG-TV-NAME">
            <stereotypes>
                <stereo required="no">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
        
        <tv id="CFG-TV-DEFINITION" origin="system">
            <stereotypes>
                <stereo required="yes">stereotype-name-relation-role</stereo> 
            </stereotypes>
        </tv>
    </tagged-values>
</tagset>