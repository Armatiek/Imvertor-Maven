<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="../../../Imvertor/cfg/tvsets/Grouping.xml"/>
    
    <name>KKG</name>
    
    <tagged-values>
        
        <!-- BP follows SBR requirements -->
        <xi:include href="../../../BiZa/cfg/tvsets/SBR.xml"/>
        
        <tv norm="space" id="DatumOpname">
            <name lang="nl">Datum opname</name>
            <name lang="en">Date recorded</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">stereotype-name-objecttype</stereo>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo> 
                <stereo required="yes">stereotype-name-referentielijst</stereo>
                <stereo required="yes">stereotype-name-referentie-element</stereo>
                <stereo required="yes">stereotype-name-union</stereo>
                <stereo required="yes">stereotype-name-complextype</stereo>
                <stereo required="yes">stereotype-name-externekoppeling</stereo>
            </stereotypes>
        </tv> 
        <tv norm="space" rules="NOC" id="IndicatieMateriëleHistorie">
            <name lang="nl">Indicatie materiële historie</name>
            <name lang="en">Indication material history</name>
            <derive>yes</derive>
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <!-- and add: -->
                <stereo required="no">stereotype-name-codelist</stereo>
                <stereo required="no">stereotype-name-relation-role</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value>Nee</value>
                <value>Zie groep</value>
            </declared-values>
            <declared-values lang="en">
                <value>Yes</value>
                <value>No</value>
                <value>See group</value>
            </declared-values>
        </tv>
        <tv norm="space" rules="NOC" id="IndicatieFormeleHistorie">
            <name lang="nl">Indicatie formele historie</name>
            <name lang="en">Indication formal history</name>
            <derive>yes</derive>
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <!-- and add: -->
                <stereo required="no">stereotype-name-codelist</stereo>
                <stereo required="no">stereotype-name-relation-role</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value>Nee</value>
                <value>Zie groep</value>
            </declared-values>
            <declared-values lang="en">
                <value>Yes</value>
                <value>No</value>
                <value>See group</value>
            </declared-values>
        </tv>
        <tv norm="space" id="IndicatieAuthentiek">
            <name lang="nl">Indicatie authentiek</name>
            <name lang="en">Indication authentic</name>
            <derive>yes</derive>
            <stereotypes>
                <!-- optional for inherited stereos -->
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <!-- and add this stereo -->
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-codelist</stereo>
                <stereo required="no">stereotype-name-relation-role</stereo>
            </stereotypes>
        </tv>
       
        <tv norm="space" id="Patroon"><!-- MIG -->
            <name lang="nl">Patroon</name>
            <name lang="en">Pattern</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-datatype</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="MogelijkGeenWaarde">
            <name lang="nl">Mogelijk geen waarde</name>
            <name lang="en">Voidable</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-voidable</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value>Nee</value>
            </declared-values>
            <declared-values lang="en">
                <value>Yes</value>
                <value>No</value>
            </declared-values>
        </tv>
        <tv norm="space" id="DataLocatie">
            <name lang="nl">Data locatie</name>
            <name lang="en">Data location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-referentielijst</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" origin="system" id="Position">
            <name lang="nl">Positie</name>
            <name lang="en">Position</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-enum</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-association-to-composite</stereo>
                <stereo required="no">stereotype-name-enum</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Lengte">
            <name lang="nl">Lengte</name>
            <name lang="en">Length</name>
            <desc>Lengte van de waarde in posities.</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Locatie"><!-- In addition to Grouping tvset -->
            <name lang="nl">Locatie</name>
            <name lang="en">Location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-codelist</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Herkomst">   <!-- in addition to SBR -->
            <stereotypes>
                <stereo required="no">stereotype-name-codelist</stereo>
                <stereo required="no">stereotype-name-relation-role</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="HerkomstDefinitie"> <!-- in addition to SBR -->
            <stereotypes>
                <stereo required="no">stereotype-name-codelist</stereo>
                <stereo required="no">stereotype-name-relation-role</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="BaseMapping">
            <name lang="nl">Base-mapping</name>
            <name lang="en">Base-mapping</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-koppelklasse</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Relation">
            <name lang="nl">Relatie</name>
            <name lang="en">Relation</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        
    </tagged-values>
</tagset>