<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="../../../Imvertor/cfg/tvsets/Grouping.xml"/>
    
    <name>KKG</name>
    
    <tagged-values>
        
        <!-- BP follows SBR requirements -->
        <xi:include href="../../../BiZa/cfg/tvsets/SBR.xml"/>
        
        <tv norm="space" id="DatumOpname">
            <name lang="nl">Datum opname</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes" lang="nl">Objecttype</stereo>
                <stereo required="yes" lang="nl">Attribuutsoort</stereo>
                <stereo required="yes" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="yes" lang="nl">Relatiesoort</stereo> 
                <stereo required="yes" lang="nl">Referentielijst</stereo>
                <stereo required="yes" lang="nl">Referentie element</stereo>
                <stereo required="yes" lang="nl">Union</stereo>
                <stereo required="yes" lang="nl">Complex datatype</stereo>
                <stereo required="yes" lang="nl">Externe koppeling</stereo>
            </stereotypes>
        </tv> 
        <tv norm="space" rules="NOC" id="IndicatieMateriëleHistorie">
            <name lang="nl">Indicatie materiële historie</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="yes" lang="nl">Attribuutsoort</stereo>
                <stereo required="yes" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="yes" lang="nl">Relatiesoort</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value>Nee</value>
                <value>Zie groep</value>
            </declared-values>
        </tv>
        <tv norm="space" rules="NOC" id="IndicatieFormeleHistorie">
            <name lang="nl">Indicatie formele historie</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="yes" lang="nl">Attribuutsoort</stereo>
                <stereo required="yes" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="yes" lang="nl">Relatiesoort</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value>Nee</value>
                <value>Zie groep</value>
            </declared-values>
        </tv>
        <tv norm="space" id="IndicatieAuthentiek">
            <name lang="nl">Indicatie authentiek</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="yes" lang="nl">Attribuutsoort</stereo>
                <stereo required="yes" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="yes" lang="nl">Relatiesoort</stereo>
            </stereotypes>
        </tv>
       
        <tv norm="space" id="Patroon"><!-- MIG -->
            <name lang="nl">Patroon</name>
            <name lang="en">Pattern</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Referentie element</stereo>
                <stereo required="no" lang="nl">Union element</stereo>
                <stereo required="no" lang="nl">Datatype</stereo>
                <stereo required="no" lang="nl">Complex datatype</stereo>
                <stereo required="no" lang="nl">Data element</stereo>
                <stereo required="no" lang="nl">Union element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="MogelijkGeenWaarde">
            <name lang="nl">Mogelijk geen waarde</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="no" lang="nl">Relatiesoort</stereo>
                <stereo required="no" lang="nl">Voidable</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value>Nee</value>
            </declared-values>
        </tv>
        
        <tv norm="space" id="DataLocatie"><!-- MIG -->
            <name lang="nl">Data locatie</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="no" lang="nl">Referentielijst</stereo>
                <stereo required="no" lang="nl">Referentie element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" origin="system" id="Position">
            <name lang="nl">Positie</name>
            <name lang="en">Position</name>
            <derive>yes</derive><!-- BP kan altijd terugvallen op een supplier BP -->
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Relatiesoort</stereo>
                <stereo required="no" lang="nl">Union element</stereo>
                <stereo required="no" lang="nl">Enum</stereo>
                <stereo required="no" lang="nl">Data element</stereo>
                <stereo required="no" lang="nl">Referentie element</stereo>
                <stereo required="no" lang="nl">Gegevensgroep compositie</stereo>
                <stereo required="no" lang="#all">#none</stereo> <!-- also allowed on enumeration values that have no enum stereo -->
            </stereotypes>
        </tv>
        
        <!--
            de volgende TVs zijn opgenomen in afwijking van basismodel BP
        -->
        <tv norm="space" id="MaximaleLengte">
            <name lang="nl">Maximale lengte</name>
            <desc></desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Data element</stereo>
                <stereo required="no" lang="nl">Union element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="MinimaleLengte">
            <name lang="nl">Minimale lengte</name>
            <desc></desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Data element</stereo>
                <stereo required="no" lang="nl">Union element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="LengteExact">
            <name lang="nl">Lengte (exact)</name>
            <desc></desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Data element</stereo>
                <stereo required="no" lang="nl">Union element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Locatie"><!-- In addition to Grouping tvset -->
            <name lang="nl">Locatie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="yes" lang="nl">Codelist</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Toelichting">
            <stereotypes>
                <stereo required="yes" lang="nl">Enumeratie</stereo>
                <stereo required="yes" lang="nl">Codelist</stereo>
                <stereo required="yes" lang="nl">Relatierol</stereo>
            </stereotypes>
        </tv> 
        <tv norm="space" id="DatumOpname">
            <stereotypes>
                <stereo required="yes" lang="nl">Codelist</stereo>
                <stereo required="yes" lang="nl">Relatierol</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" rules="NOC" id="IndicatieMateriëleHistorie">
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="no" lang="nl">Relatiesoort</stereo>
                <!-- and add: -->
                <stereo required="no" lang="nl">Codelist</stereo>
                <stereo required="no" lang="nl">Relatierol</stereo>
            </stereotypes>
            <!-- declared-values inherited -->
        </tv>
        <tv norm="space" rules="NOC" id="IndicatieFormeleHistorie">
            <stereotypes>
                <!-- optional for the following stereo's -->
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="no" lang="nl">Relatiesoort</stereo>
                <!-- and add: -->
                <stereo required="no" lang="nl">Codelist</stereo>
                <stereo required="no" lang="nl">Relatierol</stereo>
            </stereotypes>
            <!-- declared-values inherited -->
        </tv>
        <tv norm="space" id="Eigenaar">
            <stereotypes>
                <stereo required="no" lang="nl">Data element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Populatie">
            <stereotypes>
                <stereo required="no" lang="nl">Data element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Kwaliteit">
            <stereotypes>
                <stereo required="no" lang="nl">Data element</stereo>
            </stereotypes>
        </tv>
       
        <tv norm="space" id="Herkomst">
            <name lang="nl">Herkomst</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Codelist</stereo>
                <stereo required="no" lang="nl">Relatierol</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="HerkomstDefinitie">
            <stereotypes>
                <stereo required="no" lang="nl">Codelist</stereo>
                <stereo required="no" lang="nl">Relatierol</stereo>
            </stereotypes>
        </tv>
      
        <tv norm="space" id="MogelijkGeenWaarde">
                <name lang="nl">Mogelijk geen waarde</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Data element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="IndicatieAuthentiek">
            <stereotypes>
                <!-- optional for inherited stereos -->
                <stereo required="no" lang="nl">Attribuutsoort</stereo>
                <stereo required="no" lang="nl">Gegevensgroeptype</stereo>
                <stereo required="no" lang="nl">Relatiesoort</stereo>
                <!-- and add this stereo -->
                <stereo required="no" lang="nl">Data element</stereo>
                <stereo required="no" lang="nl">Codelist</stereo>
                <stereo required="no" lang="nl">Relatierol</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="BaseMapping">
            <name lang="nl">Base-mapping</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Objecttype</stereo>
                <stereo required="no" lang="nl">Koppelklasse</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Relation">
            <name lang="nl">Relatie</name>
            <name lang="en">Relation</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no" lang="nl">Relatiesoort</stereo>
            </stereotypes>
        </tv>
        
    </tagged-values>
</tagset>