                <stereo required="yes">stereotype-name-application-package</stereo>
                <stereo required="yes">stereotype-name-domain-package</stereo>
                <stereo required="yes">stereotype-name-view-package</stereo>
                <stereo required="no">stereotype-name-external-package</stereo>
                <stereo required="yes">stereotype-name-internal-package</stereo>
                <!-- release van kadaster externe schema's wordt ingegenereerd binnen Imvert2concreteSchema.xsl -->
            </stereotypes>
        </tv>
        
