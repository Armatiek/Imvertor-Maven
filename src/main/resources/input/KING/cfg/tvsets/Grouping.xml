<tagset type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="../../../Imvertor/cfg/tvsets/Grouping.xml"/>
    
    <name>Grouping for KING</name>
  
    <desc>Deviations and additions to generic grouping for KING metamodels.</desc>  
   
    <tagged-values>
        
       <tv norm="space" id="CFG-TV-ISDERIVED">
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-association-to-composite</stereo>
            </stereotypes>            
        </tv>
        <tv norm="space"  id="CFG-TV-POSITION">
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-association-to-composite</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-DATALOCATION">
            <!-- bepaal de naam van deze tagged value hier, en ken toe aan nieuwe stereo's -->
            <name lang="nl">Data locatie</name>
            <name lang="en">Data location</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-referentielijst</stereo><!-- SIM Referentielijst -->
                <stereo required="no">stereotype-name-attribute</stereo><!-- SIM Referentie element -->
            </stereotypes>
        </tv>
        
    </tagged-values>
    
</tagset>