                <value>Sa03</value>
                <!--  een asynchroon bericht dat vraagt om een asynchroon synchronisatiebericht voor de actuele gegevens -->
                <value>Sa04</value>
                <!--  een synchroon bericht dat vraagt om een synchroon synchronisatiebericht voor de actuele gegevens -->
