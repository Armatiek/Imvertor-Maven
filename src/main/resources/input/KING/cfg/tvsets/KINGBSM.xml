        <tv norm="space" validate="" rules="" id="CFG-TV-BERICHTCODE">
            <name lang="nl">Berichtcode</name>
            <derive>no</derive>
            <inherit>yes</inherit>
            <stereotypes>
                <?x stereo required="yes" lang="nl">Berichttype</stereo x?>
