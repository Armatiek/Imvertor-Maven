<tagset type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
   
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="Grouping.xml"/>
    
    <!-- KING refines grouping -->
    <xi:include href="Grouping.xml"/>

    <!-- 
        applications using this metamodel may reference cross-meta Tagged values taken from SIM
    -->
    <xi:include href="KINGUGM-cross-SIM.xml"/>

    <name>KINGUGM</name>
    
    <tagged-values>
        
        <tv norm="compact" rules="NOC" id="CFG-TV-INDICATIEMATCHGEGEVEN">
            <name lang="nl">Indicatie matchgegeven</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-relatieklasse</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
            </declared-values>
        </tv>
     
        <tv norm="space" rules="NOC" id="CFG-TV-INDICATIONAUTHENTIC">
            <name lang="nl">Indicatie authentiek</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
         </tv>
        
        <tv norm="compact" rules="NOC" id="CFG-TV-INDICATIONMATERIALHISTORY">
            <name lang="nl">Indicatie materiÃ«le historie</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo><!--SIM Attribuutsoort-->
                <stereo required="yes">stereotype-name-composite</stereo><!--SIM Gegevensgroeptype-->
                <stereo required="yes">stereotype-name-relatiesoort</stereo><!--SIM Relatiesoort-->
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
                <value>N.v.t.</value>
                <value>Ja, zie regels</value>
            </declared-values>
        </tv>
        <tv norm="compact" rules="NOC" id="CFG-TV-INDICATIONFORMALHISTORY">
            <name lang="nl">Indicatie formele historie</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo><!--SIM Attribuutsoort-->
                <stereo required="yes">stereotype-name-composite</stereo><!--SIM Gegevensgroeptype-->
                <stereo required="yes">stereotype-name-relatiesoort</stereo><!--SIM Relatiesoort-->
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
                <value>N.v.t.</value>
            </declared-values>
        </tv>
        <tv norm="space" id="CFG-TV-INDICATIEINONDERZOEK">
            <name lang="nl">Indicatie in onderzoek</name>
            <derive>yes</derive><!-- UGM mag op supplier terugvallen, maar SIM is niet een supplier voor deze config, dus expliciet de naam opnemen.-->
        </tv>
        
        <tv norm="space" validate="" rules="" id="CFG-TV-VERKORTEALIAS">
            <name lang="nl">Verkorte alias</name>
            <name lang="en">Short alias</name>
            <derive>no</derive>
            <desc lang="nl">Verkorte alias is een korte naam, die uiteindelijk gekoppeld is aan een namespace in XML schema. 
                In feite is het dus een technisch configuratie-element.</desc>
            <stereotypes>
                <stereo required="yes">stereotype-name-application-package</stereo>
                <stereo required="no">stereotype-name-base-package</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-PATTERN">
            <name lang="nl">Patroon</name>
            <name lang="en">Pattern</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo><!--SIM Attribuutsoort, Referentie element, Union element, Data element-->
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-simpletype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-FORMALPATTERN">
            <name lang="nl">Formeel patroon</name>
            <name lang="en">Formal pattern</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-simpletype</stereo>
            </stereotypes>
        </tv>
        <tv norm="compact" id="CFG-TV-VOIDABLE">
            <name lang="nl">Mogelijk geen waarde</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo><!--SIM Attribuutsoort -->
                <stereo required="no">stereotype-name-composite</stereo><!-- SIM: Gegevensgroeptype -->
                <stereo required="no">stereotype-name-relatiesoort</stereo><!-- SIM: Relatiesoort  -->
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
            </declared-values>
        </tv>
        
        <tv norm="space" id="CFG-TV-MINLENGTH">
            <name lang="nl">Minimum lengte</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-MINVALUEINCLUSIVE">
            <name lang="nl">Minimum waarde (inclusief)</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="CFG-TV-MAXVALUEINCLUSIVE">
            <name lang="nl">Maximum waarde (inclusief)</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv id="CFG-TV-DESCRIPTION">
            <derive>yes</derive>
            <stereotypes>
                <stereo>stereotype-name-objecttype</stereo>
                <stereo>stereotype-name-attribute</stereo>
                <stereo>stereotype-name-relatiesoort</stereo>
                <stereo>stereotype-name-composite</stereo>
                <stereo>stereotype-name-relatieklasse</stereo>
                <stereo>stereotype-name-referentielijst</stereo>
                <stereo>stereotype-name-referentie-element</stereo>
                <stereo>stereotype-name-enumeration</stereo>
                <stereo>stereotype-name-simpletype</stereo>
                <stereo>stereotype-name-complextype</stereo>
                <stereo>stereotype-name-union</stereo>
                
                <stereo>stereotype-name-data-element</stereo>
                <stereo>stereotype-name-union-element</stereo>
                <stereo>stereotype-name-enum</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space"  id="CFG-TV-SUBSETLABEL">
            <name lang="nl">Restriction identifier</name>
            <name lang="en">Restriction identifier</name>
            <desc>A label assigned to any construct to be used for disambiguation in subset (restriction) constructs.</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-association-to-composite</stereo>
                <stereo required="no">stereotype-name-relatieklasse</stereo>
                <stereo required="no">stereotype-name-referentielijst</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-enumeration</stereo>
                <stereo required="no">stereotype-name-simpletype</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-union</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-enum</stereo>
            </stereotypes>
        </tv>
       
    </tagged-values>
</tagset>