<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
   
    <!-- 
        this metamodel is subject to standard Grouping conventions 
    -->
    <xi:include href="../../../Imvertor/cfg/tvsets/Grouping.xml"/>
    
    <name>KINGUGM</name>
    
    <tagged-values>
        
        <tv norm="compact" rules="NOC" id="IndicatieKerngegeven">
            <name lang="nl">Indicatie kerngegeven</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-relatieklasse</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
            </declared-values>
        </tv>
        <tv norm="space" rules="NOC" id="IndicatieAuthentiek">
            <name lang="nl">Indicatie authentiek</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
         </tv>
        
        <tv norm="compact" rules="NOC" id="IndicatieMateriëleHistorie">
            <name lang="nl">Indicatie materiële historie</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo><!--SIM Attribuutsoort-->
                <stereo required="yes">stereotype-name-composite</stereo><!--SIM Gegevensgroeptype-->
                <stereo required="yes">stereotype-name-relatiesoort</stereo><!--SIM Relatiesoort-->
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
                <value>N.v.t.</value>
                <value>Ja, zie regels</value>
            </declared-values>
        </tv>
        <tv norm="compact" rules="NOC" id="IndicatieFormeleHistorie">
            <name lang="nl">Indicatie formele historie</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo><!--SIM Attribuutsoort-->
                <stereo required="yes">stereotype-name-composite</stereo><!--SIM Gegevensgroeptype-->
                <stereo required="yes">stereotype-name-relatiesoort</stereo><!--SIM Relatiesoort-->
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
                <value>N.v.t.</value>
            </declared-values>
        </tv>
        <tv norm="space" id="IndicatieInOnderzoek">
            <name lang="nl">Indicatie in onderzoek</name>
            <derive>yes</derive><!-- UGM mag op supplier terugvallen, maar SIM is niet een supplier voor deze config, dus expliciet de naam opnemen.-->
        </tv>
        
        <tv norm="space" id="IndicatieTerugrelatie">
            <name lang="nl">Indicatie terugrelatie</name>
            <name lang="en">Indication reverse relation</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="MnemonicIndicatieTerugrelatie">
            <name lang="nl">Mnemonic relatiesoort terugrelatie</name>
            <name lang="en">Mnemonic indication reverse relation</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-relatiesoort</stereo><!-- alleen verplicht als IndicatieTerugrelatie is Ja -->
            </stereotypes>
        </tv>
       
        <tv norm="space" validate="" rules="" id="VerkorteAlias">
            <name lang="nl">Verkorte alias</name>
            <name lang="en">Short alias</name>
            <derive>no</derive>
            <desc lang="nl">Verkorte alias is een korte naam, die uiteindelijk gekoppeld is aan een namespace in XML schema. 
                In feite is het dus een technisch configuratie-element.</desc>
            <stereotypes>
                <stereo required="yes">stereotype-name-application-package</stereo>
                <stereo required="no">stereotype-name-base-package</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Patroon">
            <name lang="nl">Patroon</name>
            <name lang="en">Pattern</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo><!--SIM Attribuutsoort, Referentie element, Union element, Data element-->
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-datatype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="FormeelPatroon">
            <name lang="nl">Formeel patroon</name>
            <name lang="en">Formal pattern</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo><!--SIM Attribuutsoort, Referentie element, Union element, Data element-->
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-datatype</stereo>
            </stereotypes>
        </tv>
        <tv norm="compact" id="MogelijkGeenWaarde">
            <name lang="nl">Mogelijk geen waarde</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo><!--SIM Attribuutsoort -->
                <stereo required="no">stereotype-name-composite</stereo><!-- SIM: Gegevensgroeptype -->
                <stereo required="no">stereotype-name-relatiesoort</stereo><!-- SIM: Relatiesoort  -->
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
            </declared-values>
        </tv>
        
        <tv norm="space" id="MinimumLengte">
            <name lang="nl">Minimum lengte</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="MinimumWaardeInclusief">
            <name lang="nl">Minimum waarde (inclusief)</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="MaximumWaardeInclusief">
            <name lang="nl">Maximum waarde (inclusief)</name>
            <derive>no</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="DataLocatie"><!-- MIG -->
            <name lang="nl">Data locatie</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="no">stereotype-name-referentielijst</stereo><!-- SIM Referentielijst -->
                <stereo required="no">stereotype-name-attribute</stereo><!-- SIM Referentie element -->
            </stereotypes>
        </tv>
        
        <tv norm="space"  id="Positie"><!-- origin="system" -->
            <name lang="nl">Positie</name>
            <name lang="en">Position</name>
            <derive>yes</derive><!-- Als niet ingevoerd, dan overnemen uit SIM. -->
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo><!--SIM Attribuutsoort, Union element, Data element, Referentie element-->
                <stereo required="no">stereotype-name-relatiesoort</stereo><!-- SIM: Relatiesoort  -->
                <stereo required="no">stereotype-name-enum</stereo>
                <stereo required="no">stereotype-name-association-to-composite</stereo><!-- SIM Gegevensgroep compositie-->
            </stereotypes>
        </tv>
        
    </tagged-values>
</tagset>