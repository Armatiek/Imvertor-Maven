<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
    <name>KING</name>
    <tagged-values>
        <!-- KING follows BP requirements -->
        <xi:include href="../../../BP/cfg/tvsets/BP.xml"/>
        
        <tv norm="space" id="AanduidingStrijdigheidNietigheid">
            <name lang="nl">Aanduiding strijdigheid/nietigheid</name>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="IndicatieInOnderzoek">
            <name lang="nl">Indicatie in onderzoek</name>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <desc>
                De indicatie of te bevragen is dat er twijfel is of is geweest aan de juistheid van de attribuutwaarde en dat een onderzoek wordt 
                of is uitgevoerd naar de juistheid van de attribuutwaarde. 
            </desc>
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
            </declared-values>
        </tv>
        <tv norm="space" id="CFG-TV-HERKOMSTDEFINITIE">
            <name lang="nl">Herkomst definitie</name>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <desc>
                De basisregistratie of het informatiemodel waaruit de definitie is overgenomen dan wel een aanduiding die 
                aangeeft uit welke bronnen de defintie is samengesteld.
            </desc>
            <stereotypes>
                <!-- in addition to BP -->
                <stereo required="yes">stereotype-name-referentielijst</stereo> 
                <stereo required="yes">stereotype-name-referentie-element</stereo>
                <stereo required="yes">stereotype-name-union</stereo>
                <stereo required="yes">stereotype-name-union-element</stereo>
                <stereo required="yes">stereotype-name-complextype</stereo>
                <stereo required="yes">stereotype-name-data-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="compact" rules="NOC" id="IndicatieMateriëleHistorie">
            <name lang="nl">Indicatie materiële historie</name>
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
                <value>Ja, zie regels</value>
                <value>N.v.t.</value>
            </declared-values>
        </tv>
        <tv norm="compact" rules="NOC" id="IndicatieFormeleHistorie">
            <name lang="nl">Indicatie formele historie</name>
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
                <value>Zie groep</value>
                <value>N.v.t.</value>
            </declared-values>
        </tv>
        <tv norm="space" id="IndicatieAuthentiek">
            <name lang="nl">Indicatie authentiek</name>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="compact" rules="NOC" id="MogelijkGeenWaarde">
            <name lang="nl">Mogelijk geen waarde</name>
            <stereotypes>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
            <declared-values lang="nl">
                <value>Ja</value>
                <value default="yes">Nee</value>
            </declared-values>
        </tv>
        
        <tv norm="space" id="Groepnaam">
            <name lang="nl">Groepnaam</name>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <stereotypes>
                <stereo required="no">stereotype-name-tekentechnisch</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Herkomst">
            <name lang="nl">Herkomst</name>
            <derive>yes</derive><!-- SIM kan altijd terugvallen op een supplier SIM -->
            <desc>
                De basisregistratie in wiens catalogus het objecttype is gespecificeerd (oftewel de basisregistratie waar het 
                objecttype deel van uitmaakt). Deze specificatie is toegevoegd omdat het wel duidelijk moet zijn in 
                welke (basis)registratie of informatiemodel het objecttype voorkomt (indien van toepassing).
            </desc>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
            </stereotypes>
        </tv>

        <tv norm="note" id="Populatie">   <!-- in aanvulling op SBR -->
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-obj-proxy</stereo>
            </stereotypes>
        </tv>
        <tv norm="note" id="Toelichting">   <!-- in aanvulling op SBR -->
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-obj-proxy</stereo>
                <stereo required="no">stereotype-name-att-proxy</stereo>
                <stereo required="no">stereotype-name-grp-proxy</stereo>
            </stereotypes>
        </tv>
        <tv norm="note" id="Regels">   <!-- in aanvulling op SBR -->
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-att-proxy</stereo>
                <stereo required="no">stereotype-name-grp-proxy</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Patroon">
            <name lang="nl">Patroon</name>
            <name lang="en">Pattern</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-simpletype</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
            </stereotypes>
        </tv>
        
        <!-- REDMINE #487612 -->
        <!-- Internal packages are propagated to Grouping --> 
       
    </tagged-values>
</tagset>