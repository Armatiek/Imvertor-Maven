<tagset type="config">
    
    <name>SBR</name>
    <descr>Stelsel basisregistraties</descr>
    <web>http://stelselcatalogus.logius.nl/over-de-stelselcatalogus/metadata/</web>
    
    <tagged-values xml:id="tagset">
        <tv norm="space" id="CFG-TV-SOURCE">
            <name lang="nl">Herkomst</name>
            <name lang="en">Source</name>
            <desc lang="nl">De registratie in wiens catalogus de constructie is gespecificeerd (oftewel de registratie waar de constructie deel van uitmaakt). 
                Deze specificatie is toegevoegd t.o.v. de registratie-catalogus aangezien het hier niet om een registratie gaat maar wel duidelijk moet zijn in welke 
                registratie de (verwijzing naar de) constructie voorkomt (indien van toepassing).</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">stereotype-name-objecttype</stereo>
                <stereo required="yes">stereotype-name-complextype</stereo>
                <stereo required="yes">stereotype-name-data-element</stereo>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-referentielijst</stereo>
                <stereo required="yes">stereotype-name-referentie-element</stereo>
                <stereo required="yes">stereotype-name-union</stereo>
                <stereo required="yes">stereotype-name-union-element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-SOURCEOFDEFINITION">
            <name lang="nl">Herkomst definitie</name>
            <name lang="en">Source of definition</name>
            <desc lang="nl">TODO</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">stereotype-name-objecttype</stereo>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        
        <!-- TODO Vaststellen per tagged value of deze derived moeten worden of niet. -->
        <tv norm="space" id="CFG-TV-ONWER">
            <name lang="nl">Eigenaar</name>
            <name lang="en">Owned by</name>
            <desc lang="nl">TODO</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo> <!-- TODO anders dan metamodel -->
                <stereo required="no">stereotype-name-attribute</stereo> <!-- TODO anders dan metamodel -->
                <stereo required="no">stereotype-name-composite</stereo> <!-- TODO anders dan metamodel -->
            </stereotypes>
        </tv>
        <tv norm="note" id="CFG-TV-POPULATION">
            <name lang="nl">Populatie</name>
            <name lang="en">Population</name>
            <desc lang="nl">TODO</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="CFG-TV-QUALITY">
            <name lang="nl">Kwaliteit</name>
            <name lang="en">Quality</name>
            <desc lang="nl">TODO</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
            </stereotypes>
        </tv>
        <tv norm="note" id="CFG-TV-RULES">
            <name lang="nl">Regels</name>
            <name lang="en">Rules</name>
            <desc lang="nl">TODO</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-attribute</stereo> <!-- TODO niet op KKG -->
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="note" id="CFG-TV-DESCRIPTION">
            <name lang="nl">Toelichting</name>
            <name lang="en">Description</name>
            <desc lang="nl">TODO</desc>
            <desc lang="en">TODO</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo>
                <stereo required="no">stereotype-name-referentielijst</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
            </stereotypes>
        </tv> 
        
        <!-- system defined tagged values -->
        
        <tv norm="space" id="CFG-TV-NAME">
            <name lang="nl">Naam</name>
            <name lang="en">Name</name>
            <desc lang="nl">De naam in natuurlijke of formele taal; afhankelijk van gekozen aanpak. Een alternatieve naam.</desc>
            <desc lang="en">The name in natural or formal language; depends on the chosen approach. An alternative name.</desc>
            <derive>yes</derive>
            <extract-from-notes>yes</extract-from-notes><!-- value may be read from notes field usig KKG-ISO scheme -->
            <stereotypes>
                <stereo required="no">stereotype-name-objecttype</stereo>
                <stereo required="no">stereotype-name-attribute</stereo>
                <stereo required="no">stereotype-name-relatiesoort</stereo> 
                <stereo required="no">stereotype-name-composite</stereo>
                <stereo required="no">stereotype-name-referentielijst</stereo>
                <stereo required="no">stereotype-name-codelist</stereo>
                <stereo required="no">stereotype-name-referentie-element</stereo>
                <stereo required="no">stereotype-name-union</stereo>
                <stereo required="no">stereotype-name-complextype</stereo>
                <stereo required="no">stereotype-name-externekoppeling</stereo>
                <stereo required="no">stereotype-name-enumeration</stereo>
            </stereotypes>
        </tv> 
        
        <!-- 
           Definition
        -->
        <tv norm="note" id="CFG-TV-DEFINITION" origin="system">
            <name lang="nl">Definitie</name>
            <name lang="en">Definition</name>
            <desc lang="nl">De beschrijving van de betekenis van de construct zoals gespecificeerd in de catalogus van de desbetreffende (basis)registratie of informatiemodel. </desc>
            <desc lang="en">The description of the meaning of the construct as specified in the catalog of the relevant (base) registration or information model.</desc>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">stereotype-name-objecttype</stereo>
                <stereo required="yes">stereotype-name-attribute</stereo>
                <stereo required="yes">stereotype-name-relatiesoort</stereo>
                <stereo required="yes">stereotype-name-composite</stereo>
                <stereo required="yes">stereotype-name-relatieklasse</stereo>
                <stereo required="yes">stereotype-name-referentielijst</stereo>
                <stereo required="yes">stereotype-name-referentie-element</stereo>
                <stereo required="yes">stereotype-name-enumeration</stereo>
                <stereo required="yes">stereotype-name-codelist</stereo>
                <stereo required="yes">stereotype-name-simpletype</stereo>
                <stereo required="yes">stereotype-name-complextype</stereo>
                <stereo required="yes">stereotype-name-union</stereo>
                
                <stereo required="no">stereotype-name-data-element</stereo>
                <stereo required="no">stereotype-name-union-element</stereo>
                <stereo required="no">stereotype-name-enum</stereo>
                
                <stereo required="no">stereotype-name-external-package</stereo>
                <stereo required="no">stereotype-name-view-package</stereo>
            </stereotypes>
        </tv>
    </tagged-values>
</tagset>